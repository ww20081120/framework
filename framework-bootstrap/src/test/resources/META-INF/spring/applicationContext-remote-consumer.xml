<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
                        http://www.springframework.org/schema/beans/spring-beans.xsd
                        http://code.alibabatech.com/schema/dubbo
                        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <dubbo:application name="framework-bootstrap-client" />
    <dubbo:registry address="zookeeper://127.0.0.1:2181" />
    <dubbo:consumer timeout="5000" />
    
    <dubbo:reference id="configService" interface="com.fccfc.framework.config.api.ConfigService$Iface" />
    <dubbo:reference id="taskService" interface="com.fccfc.framework.task.api.TaskService$Iface" />
</beans>